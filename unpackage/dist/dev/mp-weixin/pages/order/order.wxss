.orderContainer {
  height: 100%;
  width: 100%;
}
.orderContainer .order {
  background-color: #f5f5f5;
  width: 100%;
  height: 100%;
}
.orderContainer .order .orderItem {
  width: 100%;
  height: 340rpx;
  background-color: #fff;
  margin: 20rpx 0rpx 0rpx;
  padding: 28rpx 0rpx 0rpx 30rpx;
}
.orderContainer .order .orderItem .orderTop {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.orderContainer .order .orderItem .orderTop .orderImg {
  width: 64rpx;
  height: 64rpx;
  margin-right: 20rpx;
}
.orderContainer .order .orderItem .orderTop .orderInfo {
  width: 636rpx;
  height: 94rpx;
  border-bottom: 1rpx solid #f8f8f8;
  padding: 0rpx 26rpx 20rpx 0rpx;
}
.orderContainer .order .orderItem .orderTop .orderInfo .Info {
  height: 48rpx;
  line-height: 48rpx;
  font-size: 32rpx;
}
.orderContainer .order .orderItem .orderTop .orderInfo .Info .shopName {
  display: inline-block;
  width: 320rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.orderContainer .order .orderItem .orderTop .orderInfo .Info .dec {
  float: right;
  padding-right: 20rpx;
  font-size: 26rpx;
}
.orderContainer .order .orderItem .orderTop .orderInfo .Info .icon {
  position: relative;
  top: -12rpx;
  left: 2rpx;
  color: #999;
}
.orderContainer .order .orderItem .orderTop .orderInfo .time {
  font-size: 22rpx;
  color: #999;
}
.orderContainer .order .orderItem .orderMiddle {
  width: 636rpx;
  height: 85rpx;
  padding: 26rpx 30rpx 10rpx 84rpx;
  border-bottom: 1rpx solid #dfdfdf;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.orderContainer .order .orderItem .orderMiddle .price {
  padding-right: 20rpx;
  font-size: 26rpx;
  font-weight: bold;
}
.orderContainer .order .orderItem .footer {
  height: 97rpx;
  padding: 20rpx 32rpx;
}
.orderContainer .order .orderItem .footer .btn {
  width: 146rpx;
  height: 56rpx;
  padding: 0rpx;
  margin-right: 22rpx;
  border: 1rpx solid #2395ff;
  margin: 5rpx 20rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 56rpx;
  color: #2395ff;
  float: right;
}
.orderContainer .outOrderContainer .outLogin {
  width: 100%;
  height: 932rpx;
  overflow: hidden;
}
.orderContainer .outOrderContainer .outLogin .Imginfo {
  width: 100%;
  height: 562rpx;
  margin-top: 200rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.orderContainer .outOrderContainer .outLogin .Imginfo .Info {
  height: 40rpx;
  color: #6a6a6a;
  margin: 25rpx 0rpx 20rpx;
  font-size: 34rpx;
}
.orderContainer .outOrderContainer .outLogin .Imginfo .btn {
  width: 240rpx;
  color: #fff;
  background-color: #56d176;
  padding: 5rpx 10rpx 10rpx 10rpx;
  font-size: 28rpx;
  text-align: center;
}
.test {
  font-size: 0rpx;
}

